<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width" />
	<title>@ViewBag.Title</title>
	@Styles.Render("~/Static/css")
	@Styles.Render("~/Static/jquery-ui/css/files")

	@Scripts.Render("~/Static/modernizr/files")
	@Scripts.Render("~/Static/jquery/files")
	@Scripts.Render("~/Static/jquery-ui/js/files")
	@Scripts.Render("~/Static/jquery.validate/files")
	@Scripts.Render("~/Static/jquery.layout/files")

	@Scripts.Render("~/Static/extendjs/files")

	<script type="text/javascript" src="@Url.Content("~/Static/site/divLayout.js")"></script>

	<style type="text/css">
		.top_nav
		{
			height:50px;
			background-color:gray;
		}

		.footer_info
		{
			height:150px;
			background-color:gray;
		}
	</style>
	@RenderSection("head", required: false)
</head>
<body>
	<div class="top_nav"></div>
	@RenderSection("top", required: false)
	<div id="body">
		@RenderBody()
	</div>
	<div class="footer_info"></div>
	@RenderSection("scripts", required: false)
</body>
</html>
